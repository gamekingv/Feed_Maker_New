{
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "webextensions": true
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error"
        ],
        "no-var": "error",
        "no-console": "off"
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2017,
        "sourceType": "module"
    }
}